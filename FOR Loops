my_iterable = [1,2,3,4]
for item_name in my_iterable:
    print(item_name)


for num in my_iterable:
    if num % 2 == 0:
        print(num)
    else:
        print(f'Odd Number: {num}')



list_sum = 0
for numa in my_iterable:
    list_sum = list_sum + num
    #print (list_sum)
print (list_sum)

#si el print esta identado, esta dentro del ciclo del FOR

mystring = 'Hello World'
for letter in mystring:
    print(letter)

for letter in 'Hello World':
    print('-' + letter)



mylist = [(1,2),(3,4),(5,6),(7,8)]
for (a,b) in mylist:
    print(a)
    print(b)


